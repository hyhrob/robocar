<?xml version="1.0" encoding="utf-8"?>
<executive version="1.0" name="executive">
	<actions>
		<action>
			<type>2</type>
			<name>AT_MoveForward</name>
	  		<speed_percent>10</speed_percent>
			<control>
				<gpio pin="37" value="1"/>
				<gpio pin="35" value="1"/>
			</control>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
		<action>
			<type>8</type>
			<name>AT_HeadNod</name>
	  		<speed_percent>10</speed_percent>
			<control>
				<gpio pin="33" value="1"/>
				<gpio pin="31" value="0"/>
			</control>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
	</actions>
</executive>
