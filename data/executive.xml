<?xml version="1.0" encoding="utf-8"?>
<executive version="1.0" name="executive">
	<actions>
		<action>
			<type>1</type>
			<name>AT_Ready</name>
			<control>
				<gpio pin="37" value="0"/>
				<gpio pin="35" value="0"/>
				<gpio pin="33" value="0"/>
				<gpio pin="31" value="0"/>
			</control>
	  		<speed>
			  <range mini="0" max="100"/>
			  <default>0</default>
			</speed>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
		<action>
			<type>2</type>
			<name>AT_MoveForward</name>
			<control>
				<gpio pin="37" value="1"/>
				<gpio pin="35" value="1"/>
			</control>
	  		<speed>
			  <range mini="0" max="100"/>
			  <default>50</default>
			</speed>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
		<action>
			<type>4</type>
			<name>AT_Leftward</name>
			<control>
				<gpio pin="37" value="1"/>
				<gpio pin="35" value="0"/>
			</control>
	  		<speed>
			  <range mini="0" max="100"/>
			  <default>20</default>
			</speed>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
		<action>
			<type>5</type>
			<name>AT_Rightward</name>
			<control>
				<gpio pin="37" value="0"/>
				<gpio pin="35" value="1"/>
			</control>
	  		<speed>
			  <range mini="0" max="100"/>
			  <default>20</default>
			</speed>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
		<action>
			<type>8</type>
			<name>AT_HeadNod</name>
			<control>
				<gpio pin="33" value="1"/>
				<gpio pin="31" value="0"/>
			</control>
	  		<speed>
			  <range mini="0" max="100"/>
			  <default>50</default>
			</speed>
			<log>
				<filename>gpio_ctrl_flow</filename>
				<level>0</level>
			</log>
		</action>
	</actions>
</executive>
